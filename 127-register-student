public String registerStudent(String name, String phone, String email) {
    // code here
apxio__Student__c student2 = new apxio__Student__c();
student2.Name= name;
student2.apxio__Phone__c= phone;
student2.apxio__Email__c=email;

insert student2;

apxio__Student__c student = [SELECT Id FROM apxio__Student__c WHERE Id =: student2.Id]

return student;

}